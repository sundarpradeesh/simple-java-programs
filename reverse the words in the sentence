import java.util.*;
import java.io.*;
import java.lang.*;
public class reverse
{
	public static void main(String args[])
	{
		Scanner in=new Scanner(System.in);
		String s;
		System.out.println("enter a string");
		s=in.nextLine();
		String ss[]=s.split(" ");
		int i=0;
		for(i=ss.length-1;i>=0;i--)
		{
		System.out.print(ss[i]+" ");	
		}
		
	}
}
